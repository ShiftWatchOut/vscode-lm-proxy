{
  "name": "vscode-lm-proxy",
  "displayName": "LM Proxy",
  "description": "VSCode Language Model API Proxy with OpenAI API compatibility",
  "version": "0.0.1",
  "publisher": "vscode-lm-proxy",
  "author": {
    "name": "Ryo Nakae"
  },
  "license": "MIT",
  "homepage": "https://github.com/user/vscode-lm-proxy",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/vscode-lm-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/user/vscode-lm-proxy/issues"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Snippets"
  ],
  "keywords": [
    "language model",
    "ai",
    "llm",
    "openai",
    "proxy"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "LM Proxy",
      "properties": {
        "vscode-lm-proxy.port": {
          "type": "number",
          "default": 4000,
          "minimum": 1024,
          "maximum": 65535,
          "description": "LM Proxyサーバーが使用するポート番号"
        }
      }
    },
    "commands": [
      {
        "command": "vscode-lm-proxy.startServer",
        "title": "Start LM Proxy Server",
        "category": "LM Proxy"
      },
      {
        "command": "vscode-lm-proxy.stopServer",
        "title": "Stop LM Proxy Server",
        "category": "LM Proxy"
      },
      {
        "command": "vscode-lm-proxy.selectModel",
        "title": "Select Language Model",
        "category": "LM Proxy"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-lm-proxy.startServer",
        "key": "ctrl+shift+l s",
        "mac": "cmd+shift+l s",
        "when": "!vscode-lm-proxy.serverRunning"
      },
      {
        "command": "vscode-lm-proxy.stopServer",
        "key": "ctrl+shift+l x",
        "mac": "cmd+shift+l x",
        "when": "vscode-lm-proxy.serverRunning"
      },
      {
        "command": "vscode-lm-proxy.selectModel",
        "key": "ctrl+shift+l m",
        "mac": "cmd+shift+l m"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vscode-lm-proxy.startServer",
          "when": "!vscode-lm-proxy.serverRunning"
        },
        {
          "command": "vscode-lm-proxy.stopServer",
          "when": "vscode-lm-proxy.serverRunning"
        }
      ],
      "statusBar/remoteIndicator": [
        {
          "command": "vscode-lm-proxy.startServer",
          "when": "!vscode-lm-proxy.serverRunning"
        },
        {
          "command": "vscode-lm-proxy.stopServer",
          "when": "vscode-lm-proxy.serverRunning"
        },
        {
          "command": "vscode-lm-proxy.selectModel",
          "group": "1_model"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "test:unit": "vscode-test --extensionDevelopmentPath=. --extensionTestsPath=out/test/suite/index",
    "test:commands": "vscode-test --extensionDevelopmentPath=. --extensionTestsPath=out/test/suite/commands.test",
    "test:server": "vscode-test --extensionDevelopmentPath=. --extensionTestsPath=out/test/suite/server.test",
    "test:model": "vscode-test --extensionDevelopmentPath=. --extensionTestsPath=out/test/suite/model.test",
    "test:integration": "vscode-test --extensionDevelopmentPath=. --extensionTestsPath=out/test/suite/integration.test"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.1",
    "glob": "^11.0.3",
    "mocha": "^11.7.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "express": "^4.18.3"
  },
  "extensionDependencies": [],
  "extensionKind": [
    "ui"
  ],
  "capabilities": {
    "virtualWorkspaces": {
      "supported": false,
      "description": "この拡張機能はVSCodeのLanguage Model APIを利用するため、ローカル環境が必要です"
    },
    "untrustedWorkspaces": {
      "supported": false,
      "description": "この拡張機能はVSCodeのLanguage Model APIを利用するため、信頼されたワークスペースが必要です"
    }
  }
}
